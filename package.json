{
  "name": "krypto-swap",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "packages/**",
    "app/**"
  ],
  "scripts": {
    "app:dev": "cd app && next dev",
    "app:build": "cd app && next build",
    "app:start": "cd app && next start",
    "app:lint": "cd app && next lint",
    "app:export": "cd app && next export",
    "app:postbuild": "cd app && next-sitemap",
    "app:analyze": "cross-env ANALYZE=true yarn app:build",
    "app:analyze-server": "cross-env BUNDLE_ANALYZE=server yarn app:build",
    "app:analyze-browser": "cross-env BUNDLE_ANALYZE=browser yarn app:build",
    "clear": "rimraf ./app/.next && rimraf build && rimraf ./app/out && rimraf node_modules/.cache",
    "lint": "yarn jest --config jest-eslint.config.js",
    "lint:fix": "yarn run eslint --ext .ts,.tsx,.js . --fix",
    "ts": "yarn run tsc --noEmit"
  },
  "dependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^14.14.31",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "daisyui": "^2.24.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.3.1",
    "jest-cli": "^27.3.1",
    "jest-runner-eslint": "^1.0.0",
    "lodash": "^4.17.21",
    "next": "12.2.0",
    "prettier": "^2.4.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "typescript": "^4.3.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.2.5",
    "@tailwindcss/typography": "^0.5.7",
    "autoprefixer": "^10.4.8",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.16",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.1.8"
  }
}
